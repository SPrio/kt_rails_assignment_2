<%= form_for @image, url: images_path, html: { multipart: true  } do |form| %>
  <%= render :partial => "shared/error", locals: {var: @image} %>
  <%= form.file_field :avatar, required: true %>
  <%= form.submit %>
<% end %>
</br>
<%= link_to "Show Images", images_path %>
</br>
<%= link_to "Go back to Home", root_path %>